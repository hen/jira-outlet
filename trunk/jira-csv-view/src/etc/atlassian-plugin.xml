<atlassian-plugin key="outlet.jira.issueview.csv" name="CSV Issue View">
    <plugin-info>
        <description>CSV Issue View</description>
        <version>0.4</version>
        <application-version min="4.0" max="4.0"/>
        <vendor name="Henri Yandell" url="http://www.buildguy.com/"/>
    </plugin-info>

    <search-request-view key="searchrequest-csv-all-fields" i18n-name-key="csv-all-title" name="Csv"
class="outlet.jira.issueview.csv.SearchRequestCsvViewAllFields" state='enabled' fileExtension="csv" contentType="text/plain">
        <resource type="i18n" name="i18n" location="outlet.jira.issueview.csv.all" />
        <resource type="velocity" name="descriptionTable" location="templates/plugins/searchrequestviews/searchrequest-description-header.vm" />
        <order>15</order>
    </search-request-view>

    <search-request-view key="searchrequest-csv-current-fields" i18n-name-key="csv-current-title" name="Csv (Current)" class="outlet.jira.issueview.csv.SearchRequestCsvViewCurrentFields" state='enabled' fileExtension="csv" contentType="text/plain">
        <resource type="i18n" name="i18n" location="outlet.jira.issueview.csv.current" />
        <resource type="velocity" name="descriptionTable" location="templates/plugins/searchrequestviews/searchrequest-description-header.vm" />
        <order>15</order>
    </search-request-view>

</atlassian-plugin>
